<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Platforms>AnyCPU</Platforms>
    <PlatformTarget>AnyCPU</PlatformTarget>
    <DebugType>portable</DebugType>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <NoWin32Manifest>true</NoWin32Manifest>
    <IsPublishable>False</IsPublishable>
    <ProduceReferenceAssembly>false</ProduceReferenceAssembly>
    <Company>Christian Ohle</Company>
    <Copyright>© Christian Ohle</Copyright>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <PackageId>RationalNumerics</PackageId>
    <Authors>ChristianOhle</Authors>
    <Description>.NET library, rational arithmetic base on a virtual stack machine.</Description>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <Deterministic>False</Deterministic>
    <NoWarn>IDE1006</NoWarn>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <Deterministic>False</Deterministic>
    <NoWarn>IDE1006</NoWarn>
  </PropertyGroup>

</Project>
